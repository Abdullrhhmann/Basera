{
  "name": "basira-real-estate-server",
  "version": "1.0.0",
  "description": "Backend server for Basira Real Estate",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "start:prod": "pm2 start ecosystem.config.js --env production",
    "stop:prod": "pm2 stop basira-backend",
    "restart:prod": "pm2 restart basira-backend",
    "reload:prod": "pm2 reload basira-backend",
    "delete:prod": "pm2 delete basira-backend",
    "logs": "pm2 logs basira-backend",
    "logs:error": "pm2 logs basira-backend --err",
    "logs:out": "pm2 logs basira-backend --out",
    "status": "pm2 status",
    "monit": "pm2 monit",
    "create-admin": "node scripts/createAdmin.js",
    "create-custom-admin": "node scripts/createCustomAdmin.js",
    "seed-super-admin": "node scripts/seedBasiraSuperAdmin.js",
    "reset-admins": "node scripts/resetAdmins.js",
    "seed-properties": "node scripts/seedProperties.js",
    "seed-locations": "node scripts/seedLocations.js",
    "seed-launches": "node scripts/seedLaunches.js",
    "etl:export": "node scripts/etl/exportMongo.js",
    "etl:import": "node scripts/etl/importToPostgres.js",
    "etl:verify": "node scripts/etl/verifyCounts.js",
    "prisma:generate": "prisma generate --schema prisma/schema.prisma",
    "prisma:migrate:dev": "prisma migrate dev --schema prisma/schema.prisma",
    "prisma:migrate:deploy": "prisma migrate deploy --schema prisma/schema.prisma",
    "backup:db": "bash backup-db.sh",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@prisma/adapter-pg": "^7.0.1",
    "@prisma/client": "^7.0.1",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.19",
    "axios": "^1.12.2",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.41.0",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.7",
    "pg": "^8.16.3",
    "prisma": "^7.0.1",
    "react-helmet-async": "^2.0.5"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  },
  "keywords": [
    "real-estate",
    "nodejs",
    "express",
    "postgresql",
    "prisma"
  ],
  "author": "Basira Real Estate",
  "license": "MIT"
}
